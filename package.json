{
  "name": "hugo_site_css-grid",
  "version": "1.0.0",
  "description": "This is the Hugo version of the repo for https://brycewray.com.",
  "main": "postcss.config.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "clean": "rm -rf public",
    "start": "NODE_ENV=development npm-run-all clean --parallel dev:*",
    "build": "NODE_ENV=production npm-run-all clean --parallel prod:*",
    "testbuild": "NODE_ENV=production npm-run-all clean --parallel testbuild:*",
    "dev:postcss": "postcss ./assets/css/index.css -d public/css/ --config ./postcss.config.js -w",
    "dev:wmFetch": "node -r dotenv/config assets/js/webmentions.js",
    "dev:hugo": "hugo server",
    "prod:postcss": "postcss ./assets/css/index.css -d public/css/ --config ./postcss.config.js -w",
    "prod:wmFetch": "node assets/js/webmentions.js",
    "prod:hugo": "hugo --gc --minify",
    "testbuild:wmFetch": "node -r dotenv/config assets/js/webmentions.js",
    "testbuild:hugo": "hugo server --gc --minify"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "autoprefixer": "^9.7.5",
    "axios": "^0.19.2",
    "cssnano": "^4.1.10",
    "dot-env": "0.0.1",
    "dotenv": "^8.2.0",
    "get-json": "^1.0.1",
    "jsonfile": "^6.0.1",
    "lazysizes": "^5.2.2",
    "lodash": "^4.17.15",
    "md5": "^2.2.1",
    "node-fetch": "^2.6.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.27",
    "postcss-clean": "^1.1.0",
    "postcss-cli": "^7.1.0",
    "postcss-css-variables": "^0.14.0",
    "postcss-import": "^12.0.1",
    "postcss-nesting": "^7.0.1",
    "postcss-partial-import": "^4.1.0",
    "postcss-preset-env": "^6.7.0",
    "tailwindcss": "^1.4.6"
  },
  "engines": {
    "node": "10.x"
  },
  "devDependencies": {
    "cross-env": "^7.0.2",
    "from-env": "^1.1.4",
    "luxon": "^1.23.0"
  }
}
